function cp-remotecontainer(){
    local repo_name=""
    case "$1" in
        "haskell") repo_name=$1 ;;
        "node_14") repo_name=$1 ;;
        "yarn") repo_name=$1 ;;
        *) echo "inavalid argument"
                exit(1) ;;
    esac

    local dir="${HOME}/Workspace/github.com/hosshii/devcontainer/${repo_name}"
    cp -r ${dir}/.* ${dir}/* .
}

cp-remotecontainer "$@"
